<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	function list(){
		let arr = ["사과", "포도", "수박", "체리", "오렌지", "블루베리"];
		//배열의 모든 요소를 순서없는 목록으로 출력
		data ="1";
		for(let i = 0; i < arr.size; i++){
			data += `<li> ${arr[i]} </li>`;
			console.log(data);
		}
	}
	list();
</script>
</head>
