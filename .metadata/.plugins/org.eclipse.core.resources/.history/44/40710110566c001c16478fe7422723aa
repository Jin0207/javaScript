<title>Insert title here</title>
<script type="text/javascript">
	window.onload = function() {
		
		const hours = new Date().getHours();
		let result = document.getElementById("result").src;
		result = "img/sun.png";
		
		if(hours > 17){
			document.getElementById("result").src = "img/moon.png";	
		}
	}
</script> 
</head>
