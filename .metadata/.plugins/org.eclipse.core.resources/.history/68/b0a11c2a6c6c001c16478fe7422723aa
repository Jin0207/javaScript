<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	window.onload = function(){
		let arr = ["사과", "포도", "수박", "체리", "오렌지", "블루베리"];
		//배열의 모든 요소를 순서없는 목록으로 출력
		let data ="<ul>";
		for(let i = 0; i < arr.length; i++){
			data += "<li>" + arr[i] + "</li>";
		}
		data +="</ul>";
		document.getElementById("result").innerHTML = data;
	}
</script>
</head>
<body>
	<div id="result"></div>
</body>
</html>